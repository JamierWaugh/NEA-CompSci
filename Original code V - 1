vw1  = pieceInfo(0,7,"W",False,10,13)
vw2  = pieceInfo(2,7,"W",False,10,14)
vw3  = pieceInfo(4,7,"W",False,10,15)
vw4  = pieceInfo(6,7,"W",False,10,16)
